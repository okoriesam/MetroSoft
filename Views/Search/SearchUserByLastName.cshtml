@model IEnumerable<Metrosoft.Models.Users>

@*@page
@model Metrosoft.Views.Search.SearchUserByLastNameModel
@{
}*@



    <table>
        <thead>
             <tr>
                 <th>Id</th>
                 <th>LastName</th>
                 <th>FirstName</th>
                 <th>Email</th>
             </tr>
        </thead>
        <tbody>
           @foreach (var item in Model)
           {
              <tr>
                 <td>@item.Id</td> 
                <td>@item.LastName</td>
                <td>@item.FirstName</td>
                <td>@item.Email</td>
              </tr>
           }
        </tbody>
    </table>
